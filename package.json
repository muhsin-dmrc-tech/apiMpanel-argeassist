    "@paddle-js-models/ocr": "^4.1.1",
    "@paddle-js-models/ocrdet": "^4.1.1",
    "@sendgrid/client": "^8.1.4",
    "@sendgrid/mail": "^8.1.4",
    "bcryptjs": "^3.0.2",
    "class-transformer": "^0.5.1",
